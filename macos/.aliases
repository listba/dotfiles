alias j8="export JAVA_HOME=`/usr/libexec/java_home -v 1.8`"

alias aws="docker run --rm -it -v ~/.aws:/root/.aws -v $(pwd):/aws amazon/aws-cli"

alias pip3=/usr/bin/pip3
