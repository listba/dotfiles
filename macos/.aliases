alias cat="bat"
alias ls="ls -G"

alias fd="find . -type d -name"
alias ff="find . -type f -name"
alias grep="grep --color"

alias unexport="unset"

alias gle="git remote get-url origin | sed 's/github/gitlab/g' | xargs git remote set-url origin"

alias commit="gpgHelper; git commit"

